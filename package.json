{
  "name": "project-s",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "prisma generate && next dev -p 3100",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "prisma generate",
    "lint": "next lint && npx prettier . --check",
    "lint:fix": "next lint --fix && prettier . --write -l",
    "migrate": "prisma migrate dev",
    "postmigrate": "npx @snaplet/seed --config prisma/seed/seed.config.ts sync",
    "prepare": "husky",
    "seed": "node -r dotenv/config prisma/seed.js",
    "start": "next start"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.20.0",
    "@ant-design/nextjs-registry": "^1.0.0",
    "@faker-js/faker": "^8.4.1",
    "@heroicons/react": "^2.1.3",
    "@prisma/client": "^5.13.0",
    "@vercel/postgres": "^0.8.0",
    "antd": "^5.16.5",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.0",
    "lodash": "^4.17.21",
    "next": "14.2.3",
    "next-auth": "5.0.0-beta.17",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "0.97.2",
    "@types/bcrypt": "5.0.2",
    "@types/lodash": "4.17.0",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@vercel/style-guide": "6.0.0",
    "autoprefixer": "10.4.19",
    "commitizen": "4.3.0",
    "conventional-changelog-cli": "4.1.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.4.5",
    "dotenv-cli": "7.4.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "prisma": "5.13.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@8.15.7",
  "prisma": {
    "seed": "npx tsx prisma/seed/seed.ts"
  }
}
